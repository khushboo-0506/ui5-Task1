<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
  <Dialog id="dialogId" title="{i18n>dialogTitle}" class="sapUiResponsiveContentPadding">

    <content>
      <Text text="{i18n>contentMessage}"/>
    </content>

    <endButton>
      <Button text="{i18n>btnClose}" press="onCloseDialog"/>
    </endButton>
    
  </Dialog>
</core:FragmentDefinition>
